ading\">\n    <template v-slot:title>\n      Профиль пользователя\n      <b>{{fields.login}}</b>\n    </template>\n    <template v-slot:right>\n      <div class=\"btn-block\">\n        <button @click=\"$root.goBack()\" class=\"btn btn-secondary\">Назад</button>\n        <button @click=\"submitForm()\" class=\"btn btn-primary\">Записать</button>\n      </div>\n    </template>\n    <template v-slot:body>\n      <view1 :label=\"labels.id\">{{fields.id}}</view1>\n      <view1 :label=\"labels.login\">{{fields.login}}</view1>\n      <view1 label=\"Ключ для доступа по api\">{{fields.api_key}}</view1>\n      <view1 :label=\"labels.status\">\n        {{user_model.status.find(x => x.value == fields.status)\n        ? user_model.status.find(x => x.value == fields.status).text\n        : \"\"}}\n      </view1>\n      <view1 :label=\"labels.role\">\n        {{user_model.role.find(x => x.value == fields.role)\n        ? user_model.role.find(x => x.value == fields.role).text\n        : \"\"}}\n      </view1>\n      <b-text\n        v-model=\"fields.name\"\n        @change-model=\"validate('name', fields.name)\"\n        :label=\"labels.name\"\n        :error=\"errors.name\"\n      ></b-text>\n      <b-text\n        v-model=\"fields.surname\"\n        @change-model=\"validate('surname', fields.surname)\"\n        :label=\"labels.surname\"\n        :error=\"errors.surname\"\n      ></b-text>\n      <b-text\n        v-model=\"fields.password\"\n        @change-model=\"validate('password', fields.password)\"\n        :label=\"labels.password\"\n        :error=\"errors.password\"\n      ></b-text>\n    </template>\n  </form1>\n</template>\n\n<script>\nimport user_model from \"@/models/user\";\nimport api from \"@/config/api.js\";\nimport axios from \"axios\";\nimport base_input from \"@/mixings/base_input.js\";\nimport form1 from \"@/widgets/form/form1.vue\";\nimport submit_and_validate from \"@/mixings/submit_and_validate\";\n\nexport default {\n  mixins: [base_input, submit_and_validate],\n  components: {\n    form1\n  },\n  data: function() {\n    return {\n      loading: false,\n      labels: user_model.labels,\n      user_model: user_model,\n      validators: user_model.validators,\n      errors: new user_model.Fields().fields,\n      api: api.set_profile\n    };\n  },\n  computed: {\n    fields: {\n      get() {\n        return this.$store.getters[\"auth/profile\"];\n      },\n      set(value) {}\n    }\n  },\n  created() {},\n  methods: {\n    submit: function(fields, api) {\n      this.loading = true;\n      axios\n        .post(this.api, this.fields)\n        .then(response => {\n          this.loading = false;\n          this.fields = response.data.data;\n          this.$store.commit(\"auth/updateProfile\", this.fields);\n          this.$root.$emit(\"show-info\", {\n            text: \"Данные записаны\",\n            class: \"info\"\n          });\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/signup"}]}