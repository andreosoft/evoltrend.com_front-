ios\n        .post(this.api, this.data)\n        .then(response => {\n          this.loading = false;\n          this.$store.commit(\"auth/updateProfile\", this.data);\n          this.$root.$emit(\"show-info\", {\n            text: \"Данные записаны\",\n            class: \"info\"\n          });\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    }\n  }\n};\n</script>\n"]}]}